[Unit]
Description = vslam daemon

[Service]
ExecStart = /usr/bin/vslam -c @CONFIG_PATH@

#Environment="LD_LIBRARY_PATH=/path/to/cudnn:$LD_LIBRARY_PATH"

Restart = always
